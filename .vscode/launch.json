{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: biodata assistant",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/demo.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/backend/.venv/bin/python",
            "env": {
                "PYDEVD_WARN_EVALUATION_TIMEOUT": "500"
            },
            "args": [
                "--show-browser",
                "--max-issues",
                "3",
                "--demo"
            ]
        }
    ]
}